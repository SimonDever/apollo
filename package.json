{
	"name": "apollo",
	"productName": "Apollo",
	"description": "Desktop movie organiser",
	"version": "0.2.0",
	"author": "Simon Dever",
	"copyright": "Â© 2019, Simon Dever",
	"private": true,
	"license": "UNLICENSED",
	"main": "app/app.js",
	"updater": {
		"url": "https://raw.githubusercontent.com/simondever/apollo/electron-upgrade/updates.json"
	},
	"build": {
		"appId": "com.github.simondever.apollo",
		"mac": {
			"icon": "assets/images/mac-icon.icns"
		},
		"files": [
			"app/**/*",
			"node_modules/**/*",
			"package.json",
			"!updates.json",
			"!publisher.json"
		],
		"win": {
			"target": "squirrel"
		},
		"squirrelWindows": {
			"iconUrl": "https://github.com/SimonDever/Apollo/blob/master/src/assets/images/logo.png"
		}
	},
	"scripts": {
		"ng": "ng",
		"serve": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"electron": "electron .",
		"debug-electron": "./node_modules/electron/dist/electron.exe ./ --inspect",
		"start": "ng build && ./node_modules/electron/dist/electron.exe ./",
		"packager": "node_modules/.bin/electron-packager .",
		"package": "ng build && electron-builder",
		"electron-builder": "electron-builder",
		"publish": "node_modules/.bin/publish"
	},
