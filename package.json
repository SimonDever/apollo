		"@ngrx/effects": "^8.3.0",
		"@ngrx/entity": "^8.3.0",
		"@ngrx/router-store": "^8.3.0",
		"@ngrx/store": "^8.3.0",
		"@primer/octicons": "^9.1.1",
		"@types/nedb": "^1.8.9",
		"bootstrap": "^4.1.2",
